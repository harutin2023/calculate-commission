{
    "name": "harutin1991/commission-rate-test-task",
    "description": "Refactoring code",
    "type": "project",
    "config": {
        "platform": {
            "php": "8.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "8.2",
        "ext-json": "*",
        "psr/container": "^1.1 || ^2.0",
        "php-di/invoker": "^2.0",
        "laravel/serializable-closure": "^1.0",
        "symfony/filesystem": "^5.1",
        "symfony/http-client": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
